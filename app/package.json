{
  "name": "app",
  "private": true,
  "scripts": {
    "start": "meteor run -p 3020 --settings ../mup/settings.json",
    "debug": "meteor run -p 3020 --inspect=192.168.1.6 --settings ../mup/settings.json",
    "test": "meteor test -p 3022 --driver-package=practicalmeteor:mocha --settings ../mup/settings.json",
    "deploy": "echo \"<template name='version'>\n<div class='uk-container uk-section'>\" > client/version.html; git log -1 --pretty=format:\"<h2>Last Commit: %aD</h2>\n<table class='uk-table uk-table-small uk-table-striped'>\n<thead><tr><th>Hash</th><th>Description</th></tr></thead>\" >>client/version.html; git log -5 --pretty=format:\"<tr><td>%h</td><td>%s</td></tr>\" >>client/version.html; echo \"</table></div>\n</template>\" >> client/version.html; cd ../mup; mup deploy"
  },
  "dependencies": {
    "@google/maps": "^0.4.5",
    "babel-runtime": "^6.26.0",
    "cheerio": "^0.22.0",
    "lunr": "^2.1.4",
    "meteor-node-stubs": "^0.3.2",
    "moment": "^2.13.0",
    "nightmare": "^2.10.0",
    "numeral": "^1.5.3",
    "uikit": "^3.0.0-beta.35",
    "webfontloader": "^1.6.28"
  }
}
